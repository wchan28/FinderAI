{
  "name": "docora",
  "private": true,
  "version": "1.0.10",
  "scripts": {
    "dev": "vite",
    "build:web": "tsc && vite build",
    "build": "tsc -p electron/tsconfig.json && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite --port 5173\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "vite build && electron-builder",
    "build:backend:mac": "cd .. && ./scripts/build-backend-mac.sh",
    "build:backend:win": "cd .. && powershell -ExecutionPolicy Bypass -File ./scripts/build-backend-win.ps1",
    "build:all:mac": "npm run build:backend:mac && npm run build",
    "build:all:win": "npm run build:backend:win && npm run build",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "main": "dist-electron/main.js",
  "dependencies": {
    "@clerk/clerk-react": "^5.25.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@supabase/supabase-js": "^2.49.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "lucide-react": "^0.462.0",
    "mime-types": "^2.1.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.3",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "build": {
    "appId": "com.docora.app",
    "productName": "Docora",
    "artifactName": "${productName}-${arch}.${ext}",
    "publish": {
      "provider": "github",
      "owner": "wchan28",
      "repo": "FinderAI",
      "releaseType": "release"
    },
    "protocols": {
      "name": "Docora",
      "schemes": [
        "docora"
      ]
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon.icns",
      "identity": "Warren Chan (XHL625JL78)",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "notarize": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "extraResources": [
        {
          "from": "python-dist/mac",
          "to": "python-backend",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icon.ico",
      "artifactName": "${productName}.${ext}",
      "extraResources": [
        {
          "from": "python-dist/win",
          "to": "python-backend",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "dmg": {
      "background": "assets/dmg-background.png",
      "iconSize": 100,
      "window": {
        "width": 540,
        "height": 380
      },
      "contents": [
        {
          "x": 140,
          "y": 190,
          "type": "file"
        },
        {
          "x": 400,
          "y": 190,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "afterPack": "./scripts/fix-python-framework.cjs",
    "afterSign": "./scripts/notarize.cjs",
    "afterAllArtifactBuild": "./scripts/fix-mac-zip.cjs"
  },
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "eslint": "^9.10.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "postcss": "^8.5.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.2",
    "vite": "^5.4.21",
    "wait-on": "^8.0.1"
  }
}
